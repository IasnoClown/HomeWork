print(9**0.5*5) # 1st program "Арифметика"
print(9.99>9.98 and 1000 != 1000.1) # 2st program "Сравнение, or, and"
print((2*2+2),
    (2*2+2),
    (2*2+2)==(2*(2+2))) # 3rd program "Школьная загадка"
print() # 3rd program "Сложная арифметика" - Не получается
print(int(float('123.456')*10)%10) # 4th program "Первый после точки"
